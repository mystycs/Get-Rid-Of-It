<div class="jumbotron"><h2> My Listings: </h2>

<% @listings.each_with_index do |listing, index| %>
  <% if index > 0 %><p>---</p><%end%>
  <a href="/listings/<%=listing.id%>"><b><%= listing.title %></b></a> - $<%= listing.price %> - <a href="/listings/<%=listing.id%>/edit">(edit)</a> <br />
  Location: <%= listing.location %><br />
  Description: <%= listing.description %><br />
<% end %>
</div>
